{
    "manifest_version": 3,
    "name": "CA6 LiveTS",
    "description": "Timestamp creation tool for Youtube stream. By CA6.DEV",
    "version": "0.2.0",
    "permissions": [
        "activeTab",
        "downloads"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"], 
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_icon": "128.png"
    }
}
